{
  "expo": {
    "name": "counter",
    "assetBundlePatterns": ["assets/*"],
    "slug": "counter",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "counter",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#27187E"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.codedegen.counter",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": ["audio"]
      }
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "bbe4816c-87db-40fd-b4eb-cf7274e754fb"
      }
    },
    "plugins": ["expo-router", "expo-sqlite"],
    "updates": {
      "enabled": true
    }
  }
}
